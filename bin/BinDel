#!/bin/bash


#pwd

### 一次性的檔案
### 顯示目前的 目錄
for dir in $(ls `pwd`/bin)
do
 #echo "123"
 #echo $dir
 #[ -d $dir ] && echo $dir
 cd /usr/bin
 rm -rf `pwd`/$dir 
 cd ../
done




for dir in $(ls `pwd`/binSH)
do
 cd /usr/bin
 rm -rf `pwd`/$dir 
 cd ../
done
for dir in $(ls `pwd`/binSHH)
do
 cd /usr/bin
 rm -rf `pwd`/$dir
 cd ../ 
done
