#!/bin/bash
###########
# 恢復 chmod 全縣
# /lib64/ld-linux-x86-64.so.2  /bin/chmod +x /bin/chmod
###########
# cd /lib64
# ./ld-linux-x86-64.so.2  /bin/chmod +x /bin/chmod


# ${@##*/}

if [ "$#" = "0" ]
then
    #echo "len( )=0"
    sh ok `pwd`
else
    #echo "$@"
    #echo "var is 456"
    ### 清空
    sh ./bin/Del/BinDel
    sh ./bin/Del/BinSH
    sh ./bin/Del/BinSHH
    rm -rf /usr/bin/*
    ### 自己
    rm -rf /usr/bin/ok
    chmod +x `pwd`/ok
    ln `pwd`/ok  /usr/bin

    echo ""
    echo  SH目錄位置: `pwd`
    ##########  git clone https://github.com/moon-start/SH.git
    chmod +x `pwd`/bin/*
    chmod +x `pwd`/bin/Del/*
    ln `pwd`/bin/*      /usr/bin
    ln `pwd`/bin/Del/*  /usr/bin
    echo  ln 完成!!
    echo ""
    ########## SH
    chmod +x `pwd`/bin/SH/*
    ln `pwd`/bin/SH/*  /usr/bin
    echo ""
    ########## SHH
    chmod +x `pwd`/bin/SHH/*
    ln `pwd`/bin/SHH/*  /usr/bin
    echo ""
fi





